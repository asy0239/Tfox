<?xml version="1.0" encoding="UTF-8" ?>

<!-- DTD 선언 -->
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="UserList">
	<select id="userList" resultType="User"  parameterType="list">
		select 
			   user_id,
       	       user_name,
     		   user_loginid,
     		   user_date,
     		   phone,
     		   user_email
          from user_table
	</select>
	
</mapper>